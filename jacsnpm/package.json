{
  "name": "jacsnpm",
  "version": "0.1.0",
  "description": "JACS Node.js bindings with MCP integration",
  "main": "index.js",
  "types": "index.d.ts",
  "overrides": {
    "body-parser": "^2.2.1",
    "qs": "^6.14.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./a2a": {
      "import": "./src/a2a.js",
      "require": "./src/a2a.js"
    },
    "./mcp": {
      "import": "./mcp.ts"
    },
    "./http": {
      "import": "./http.js",
      "types": "./http.d.ts"
    }
  },
  "scripts": {
    "build": "napi build --platform --release && npm run build:ts",
    "build:debug": "napi build --platform && npm run build:ts",
    "build:ts": "tsc",
    "install": "npm run build",
    "test": "mocha test/**/*.test.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@napi-rs/cli": "^2.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "sinon": "^21.0.1"
  },
  "napi": {
    "name": "jacsnpm",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "files": [
    "index.js",
    "index.d.ts",
    "mcp.ts",
    "http.js",
    "http.d.ts"
  ]
}
